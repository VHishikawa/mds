    "import json\n",
    "p=Producer({'bootstrap.servers':'kafka-broker:9092'})\n",
    "m=json.dumps('eee')\n",
    "p.poll(1)\n",
    "        \n",
    "p.produce('teste1', m.encode('utf-8'))\n",
    "p.flush()"
